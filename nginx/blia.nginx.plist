<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>blia.nginx</string>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <false/>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/nginx</string>
        <string>-p</string>
        <string>/Users/blia/Projects</string>
        <string>-c</string>
        <string>/Users/blia/.nginx.conf</string>
        <string>-g</string>
        <string>daemon off;</string>
    </array>
  </dict>
</plist>
